<p id="notice"><%= notice %></p>

<p>
  <strong>Content:</strong>

  <%= @post.content %>
  <%= image_tag @post.image_url.to_s %>
</p>
<p>
    <% @post.products.each do |product| %>
      <%= product.goods_name %>
      <%= image_tag product.image_url.to_s %>
    <% end %>
</p>
<p>
  <%= @brand.name%>
</p>
<p>
  <%= @category.name%>
</p>


<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
