<h1>Listing qynqyns</h1>

<table>
  <thead>
    <tr>
      <th>Count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qynqyns.each do |qynqyn| %>
      <tr>
        <td><%= qynqyn.count %></td>
        <td><%= link_to 'Show', qynqyn %></td>
        <td><%= link_to 'Edit', edit_qynqyn_path(qynqyn) %></td>
        <td><%= link_to 'Destroy', qynqyn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qynqyn', new_qynqyn_path %>
